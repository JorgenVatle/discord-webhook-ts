{
  "compilerOptions": {
    "target": "es6",
    "rootDirs": [
      "src"
    ],
    "outDir": "dist",
    "module": "commonjs",
    "declaration": false,
    "strict": true,
    "esModuleInterop": true,
    "lib": [
      "es6",
      "DOM"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "types": [],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "types",
    "dist"
  ]
}
